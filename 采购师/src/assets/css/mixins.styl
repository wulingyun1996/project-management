bd(color, width = 1px)
  border width solid color;/*no*/
bd-t(color, width = 1px)
  border-top width solid color;/*no*/
bd-b(color, width = 1px)
  border-bottom width solid color;/*no*/
bd-r(color, width = 1px)
  border-right width solid color;/*no*/
bg(url)
  background url('./images/' + url) no-repeat center
  background-size 100% 100%
border-radius(num)
  border-radius num
  overflow hidden
f10()
  font-size 20px;/*px*/
f11()
  font-size 22px;/*px*/
f12()
  font-size 24px;/*px*/
f13()
  font-size 26px;/*px*/
f14()
  font-size 28px;/*px*/
f15()
  font-size 30px;/*px*/
f16()
  font-size 32px;/*px*/
f18()
  font-size 36px;/*px*/
f20()
  font-size 40px;/*px*/
fz(size)
  font-size 2*size;/*px*/
flex-f()
  display flex
flex-s-btw()
  flex-f()
  justify-content space-between
flex-align-item()
  flex-s-btw()
  align-items center
flex-f-align()
  flex-f()
  align-items center
flex-center()
  flex-f()
  align-items center
  justify-content center
flex-col()
  display flex
  flex-direction column
flex-col-btw()
  flex-col()
  justify-content space-between
foot-btn()
  width 100%
  height 100px
  line-height 100px
  text-align center
  color #fff
  font-size 32px;/*px*/
  background #ff8a00
foot-flex()
  height 100px
  line-height 100px
  text-align center
  color #fff
  font-size 32px;/*px*/
  background #ff8a00
fc()
  background #bcbcbc!important
  pointer-events none
pf(val)
  width 100%
  position fixed
  height val
tc()
  text-align center
tr()
  text-align right
text-over(num)
  overflow hidden
  text-overflow ellipsis
  display -webkit-box!important
  -webkit-box-orient vertical!important
  -webkit-line-clamp num
  word-break break-all
wb()
  white-space normal
  word-wrap break-word
  word-break break-all
wh(val)
  width val
  height val

